{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-extra-semi": 0,
      "no-shadow": "off",
      "linebreak-style": 0,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": 0,
      "no-restricted-globals": 0,
      "no-undef": 0,
      "consistent-return": 0,
      "no-prototype-builtins": 0,
      "import/no-extraneous-dependencies": 0,
      "class-methods-use-this": 0,
      "no-new": 0,
      "no-return-assign": 0,
      "import/prefer-default-export": 0,
      "eol-last": 0,
      "no-multiple-empty-lines": ["error", { "max": 5, "maxEOF": 0 }],
      "prefer-destructuring": ["error", {"object": true, "array": false}]

  },
      "plugins": ["jasmine"]
}
